opam-version: "1"
maintainer: "nbraud"
homepage: "http://gmplib.org/"
license: "GPL"
build: [
  ["sh" "-c" "cc -c $CFLAGS test.c"] { os != "openbsd" & os != "freebsd" }
  ["sh" "-c" "cc -c $CFLAGS -I/usr/local/include test.c"] { os = "openbsd" | os = "freebsd" }
]
depexts: [
  [["debian"] ["libgmp-dev"]]
  [["ubuntu"] ["libgmp-dev"]]
  [["osx" "homebrew"] ["gmp"]]
  [["centos"] ["gmp" "gmp-devel"]]
  [["openbsd"] ["gmp"]]
  [["freebsd"] ["gmp"]]
]
